---
import Link from "../components/Link.astro"
import "../styles.css"
import { Image, Picture } from "astro:assets"
import Ayoub from "../image/Ayoub.jpg"
import kofi from "../image/Ko-Fi.png"
import yt from "../image/youtube.png"
import cashapp from "../image/CashApp.png"
import venmo from "../image/Venmo.png"
import paypal from "../image/Paypal.png"
import contact from "../image/Contactcard.png"
import matrix from "../image/Matrix.png"
import tampashutter from "../image/Tampashutter.png"
import odysee from "../image/Odysee.png"
import { Icon } from "astro-icon/components"
import { ViewTransitions } from "astro:transitions"
import actionlens from "../image/ActionLens.png"
import patreon from "../image/Patreon.png"
import sms from "../image/SMS.svg"
import signal from "../image/signal.svg"
import abm from "../image/ABM.webp"
---

<!doctype html>
<html lang="en" class="box-border">
  <head>
    <!-- From AI -->
    <!-- Add OpenGraph and Twitter meta tags -->
    <meta property="og:title" content="Ayoub Abedrabbo â€“ Photographer & Videographer" />
    <meta property="og:description" content="Tampa-based event photographer & videographer. Book or support me here." />
    <meta property="og:image" content={Ayoub.src} />
    <meta property="og:url" content="https://www.ayoubabed.xyz/" />
    <meta name="twitter:card" content="summary_large_image" />
    <!-- Preload font -->
    <link rel="preload" href="https://rsms.me/inter/inter.css" as="style" />
    <!-- End FROM AI -->
    <ViewTransitions />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Ayoub Abedrabbo</title>
    <meta
      name="description"
      content="Ayoub Abedrabbo is an event photographer that is local to Tampa, Florida. He takes photos and videos for all budgets. Vist all his social media pages from here."
    />
    <script src="https://kit.fontawesome.com/e8d1b35a19.js" crossorigin="anonymous"></script>
    <link rel="preconnect" href="https://rsms.me/" />
    <link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
  </head>

  <body class="mx-auto my-8 h-full max-w-[90%] justify-center bg-gradient-to-t from-emerald-800 to-green-700 pb-8 text-center text-lg subpixel-antialiased lg:max-w-screen-md">
    <div class="mx-auto size-40 overflow-hidden rounded-full drop-shadow-md">
      <Image src={Ayoub} alt="Portrait of Ayoub Abedrabbo" class="h-44 object-cover object-center drop-shadow-lg" decoding="async" loading="eager" />
      <!-- <Picture src={Ayoub} alt="Portrait of Ayoub Abedrabbo" decoding="async" loading="lazy" format="['avif','webp']" class="h-44 object-cover object-center drop-shadow-md" /> -->
    </div>

    <h1 class="my-4 text-2xl font-bold text-white drop-shadow-lg">Ayoub Abedrabbo</h1>
    <p class="mx-auto my-4 text-center text-white">I am a photographer/videographer. I also make videos about many topics like Islam, manifestation, and books that im reading.</p>
    <!-- icons -->
    <div class="my-4 flex flex-wrap items-center justify-center text-2xl text-white">
      <a id="instagram" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://instagram.com/ayoubabedrabbo"><i class="fa-brands fa-instagram"></i></a>
      <a id="discord" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://discordapp.com/users/279467079990640641"
        ><i class="fa-brands fa-discord"></i></a
      >
      <a id="linkedin" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://www.linkedin.com/in/ayoubabedrabbo/"
        ><i class="fa-brands fa-linkedin"></i></a
      >
      <a id="mastodon" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://mastodon.social/@ayoubabedrabbo"><i class="fa-brands fa-mastodon"></i></a>
      <!-- <a id="etsy" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://metroviewposters.etsy.com/"><i class="fa-brands fa-etsy"></i></a> -->
      <a id="github" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://github.com/Today20092"><i class="fa-brands fa-github"></i></a>
      <a id="telegram" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://t.me/AyoubAb1"><i class="fa-brands fa-telegram"></i></a>
      <a id="youtube" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://www.youtube.com/@ayoubthegreat"><i class="fa-brands fa-youtube"></i></a>
      <a id="tiktok" target="_blank" class="mx-2 transition hover:scale-150 focus:scale-150 active:scale-150" href="https://www.tiktok.com/@ayoubabedrabbo"><i class="fa-brands fa-tiktok"></i></a>
    </div>
    <!-- link buttons -->

    <!-- Contact me section -->
    <h2 class="mb-4 mt-8 text-lg font-bold tracking-wide text-white drop-shadow-lg">Contact me</h2>
    <Link linktxt="Download my contact card" bgcolor="bg-sky-500" link="https://www.ayoubabed.xyz/AyoubA-Contact-Card.vcf" linkcolor="text-white" id="contact">
      <Image src={contact} alt="Contact Card logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Message me on Matrix" bgcolor="bg-sky-600" link="https://matrix.to/#/@ayoubab:matrix.org" linkcolor="text-white" id="matrix">
      <Image src={matrix} alt="Matrix logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Message me on Signal" bgcolor="bg-sky-700" link="https://signal.me/#eu/69JqyX9IFc7IdHHGVi0WbrS68tgkCS6duJSWkivyLk1av_6Vo2xEoLIvhn0duI7B" linkcolor="text-white" id="matrix">
      <Image src={signal} alt="Signal logo" class="link-image h-auto fill-white object-cover object-center" />
    </Link>
    <Link linktxt="Send me a text" bgcolor="bg-sky-800" link="sms:+18134240606?body=Hello Ayoub from Action Lens Media, It was nice to meet you." linkcolor="text-white" id="sms">
      <Image src={sms} alt="" class="link-image h-auto object-cover object-center" />
    </Link>

    <!--  Patreon  -->
    <h2 class="mb-4 mt-8 text-lg font-bold tracking-wide text-white drop-shadow-lg">Patreon</h2>
    <Link linktxt="Join my community on Patreon" bgcolor="bg-neutral-50" link="https://patreon.com/AyoubAbedrabbo" linkcolor="text-black" id="patreon">
      <Image src={patreon} alt="Patreon logo" class="link-image h-auto object-cover object-center" />
    </Link>

    <!-- youtube buttons -->
    <h2 class="mb-4 mt-8 text-lg font-bold tracking-wide text-white drop-shadow-lg">Watch on Youtube</h2>
    <Link linktxt="Subscribe to my Youtube Channel" bgcolor=" bg-red-700 " link="https://www.youtube.com/@ayoubthegreat" linkcolor="text-white" id="youtube-personal">
      <Image src={yt} alt="Youtube logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Visit AlphaBravoMedia Youtube Channel" bgcolor=" bg-red-800 " link="https://youtube.com/@alphabravomedia" linkcolor="text-white" id="youtube-alphabravomedia">
      <Image src={yt} alt="Youtube logo" class="link-image h-auto object-cover object-center" />
    </Link>

    <h2 class="mb-4 mt-8 text-lg font-bold tracking-wide text-white drop-shadow-lg">My video & photo company</h2>
    <Link linktxt="AlphaBravoMedia Homepage" bgcolor=" bg-orange-700 " link="https://linktr.ee/AlphaBravoMedia" linkcolor="text-white" id="alphabravomedia">
      <Image src={abm} alt="AlphaBravoMedia logo" class="link-image h-auto object-cover object-center" />
    </Link>

    <!-- <Link linktxt="My Photography Channel" bgcolor="bg-red-800" link="https://www.youtube.com/@ayoubphotos" linkcolor="text-white" id="youtube-photography"> -->
    <!-- <Image src={yt} alt="Youtube logo" class="link-image h-auto object-cover object-center" /> -->
    <h2 class="mb-4 mt-8 text-lg font-bold tracking-wide text-white drop-shadow-lg">Send me money on:</h2>
    <!-- <Link linktxt="Ko-Fi" bgcolor="bg-amber-500" link="https://ko-fi.com/ayoubab1" linkcolor="text-white" border="hover:border-amber-300 focus:border-amber-300 active:border-amber-300" id="ko-fi">
      <Image src={kofi} alt="Ko-Fi logo" class="link-image h-auto object-cover object-center" />
    </Link> -->
    <Link linktxt="CashApp" bgcolor="bg-green-700" link="https://cash.app/ayoubab1" linkcolor="text-white" id="cash-app" border="target:link-id">
      <Image src={cashapp} alt="Cashapp logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Venmo" bgcolor="bg-green-600" link="https://venmo.com/u/Ayoub-Abedrabbo" linkcolor="text-white" id="venmo">
      <Image src={venmo} alt="Venmo logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Paypal" bgcolor="bg-green-500" link="http://paypal.me/AyoubAbedrabbo" linkcolor="text-white" id="paypal">
      <Image src={paypal} alt="Paypal logo" class="link-image h-auto object-cover object-center" />
    </Link>

    <!-- Odysee Page --><!-- <h2 class="mb-4 mt-8 text-lg tracking-wide text-white drop-shadow-lg">Watch on Odysee</h2>
    <Link linktxt="Odysee Channel" bgcolor="bg-orange-700" link="https://odysee.com/@AyoubTheGreat:1" linkcolor="text-white" id="odysee">
      <Image src={odysee} alt="Odysee Logo" class="link-image h-auto object-cover object-center" />
    </Link> --><!-- My website section
     
    <h2 class="mb-4 mt-8 text-lg tracking-wide text-white drop-shadow-lg">
      My Website
    </h2>
    <Link
      linktxt="My Photography Website"
      bgcolor="bg-zinc-700"
      link="https://tampashutter.net"
      linkcolor="text-white"
      id="tampashutter"
    >
      <Image
        src={tampashutter}
        alt="Tampashutter logo"
        class="link-image h-auto object-cover object-center"
      />
    </Link> --><!-- Next section of link buttions --><!-- Action Lens Media --><!-- <h2 class="mb-4 mt-8 text-lg tracking-wide text-white drop-shadow-lg">Action Lens Media</h2> --><!-- <Link linktxt="Action Lens Media Instagram" bgcolor="bg-cyan-500" link="https://www.instagram.com/action.lensmedia" linkcolor="text-white" id="actionlensinstagram">
      <Image src={actionlens} alt="Action Lens Media Logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Action Lens Media Facebook Page" bgcolor="bg-cyan-600" link="https://www.facebook.com/people/Action-Lens-Media/61558442317195/" linkcolor="text-white" id="actionlensfacebook">
      <Image src={actionlens} alt="Action Lens Media Logo" class="link-image h-auto object-cover object-center" />
    </Link>
    <Link linktxt="Action Lens Media Ko-Fi" bgcolor="bg-cyan-700" link="https://ko-fi.com/actionlensmedia" linkcolor="text-white" id="actionlenskofi">
      <Image src={kofi} alt="Ko-Fi Logo" class="link-image h-auto object-cover object-center" />
    </Link> --><!-- Donate to me  -->
    <footer class="mt-auto w-full max-w-screen-md px-4 py-6 text-center text-sm text-white/70">
      <p class="mb-2">&copy; 2024 Ayoub Abedrabbo. All rights reserved.</p>
      <p>
        Built with <a href="https://astro.build" target="_blank" rel="noopener" class="underline hover:text-white">Astro</a>
        &amp; <a href="https://tailwindcss.com" target="_blank" rel="noopener" class="underline hover:text-white">Tailwind CSS</a>.
      </p>
    </footer>
  </body>
</html>
